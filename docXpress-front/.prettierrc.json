{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 200,
  "tabWidth": 2,
  "singleQuote": true,
  "quoteProps": "consistent",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "semi": true,
  "vueIndentScriptAndStyle": true,
  "trailingComma": "all"
}

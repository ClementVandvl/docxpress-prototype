{
  "manifest_version": 3,
  "name": "DocXpress",
  "description": "DocXpress extension allows you to quickly access your documents and automatically fill out forms.",
  "version": "1.0",
  "action": {
    "default_popup": "main.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icon.png"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
